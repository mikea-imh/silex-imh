<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title>{% block title %}{% endblock %} | InMotion Hosting</title>

    <meta name="description" content="{% block description %}{% endblock %}" />
    <meta name="keywords" content="{% block keywords %}{% endblock %}" />
    <meta name="author" content="" />

    <meta name="viewport" content="width=device-width">

    <link rel="stylesheet" href="{{ app.request.basepath }}/assets/css/styles.css">

    <script type="text/javascript">
      (function() {
        var config = {
          kitId: 'tkh8ukm',
          scriptTimeout: 3000
        };
        var h=document.getElementsByTagName("html")[0];h.className+=" wf-loading";var t=setTimeout(function(){h.className=h.className.replace(/(\s|^)wf-loading(\s|$)/g," ");h.className+=" wf-inactive"},config.scriptTimeout);var tk=document.createElement("script"),d=false;tk.src='//use.typekit.net/'+config.kitId+'.js';tk.type="text/javascript";tk.async="true";tk.onload=tk.onreadystatechange=function(){var a=this.readyState;if(d||a&&a!="complete"&&a!="loaded")return;d=true;clearTimeout(t);try{Typekit.load(config)}catch(b){}};var s=document.getElementsByTagName("script")[0];s.parentNode.insertBefore(tk,s)
      })();
    </script>

    <script src="{{ app.request.basepath }}/js/libs/modernizr-2.5.3-respond-1.1.0.min.js"></script>
</head>
<body>
<!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->

    {% set active = active|default(null) %}
    <div id="skip">
        <a href="#main">Skip to Main Content</a>
        <a href="#nav">Skip to Navigation</a>
    </div>
    <header role="banner">
        <div class="container">
            <a href="{{ path('homepage') }}"><img src="{{ app.request.basepath }}/img/logo-imh.png" alt="InMotion Hosting" /></a>
            <ul class="nav nav-pills">
                <li><a href="#">Live Chat</a></li>
                <li>888.321.HOST</li>
                <li><a href="">AMP Login</a></li>
            </ul>
        </div>
    </header>
    <nav id="nav" class="navbar navbar-static-top">
        <div class="navbar-inner">
            <div class="container">
                <ul class="nav">
                    <li class="dropdown{% if 'account' == active %} active{% endif %}">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Web Hosting</a>
                        <ul class="dropdown-menu">
                            <li {% if 'business_hosting' == active %}class="active"{% endif %}><a href="{{ path('business_hosting') }}">Business Hosting</a></li>
                            <li {% if 'vps_hosting' == active %}class="active"{% endif %}><a href="{{ path('vps_hosting') }}">VPS Hosting</a></li>
                            <li {% if 'dedicated_servers' == active %}class="active"{% endif %}><a href="{{ path('dedicated_servers') }}">Dedicated Servers</a></li>
                        </ul>
                    </li><!-- /.dropdown -->
                    <li {% if 'domains' == active %}class="active"{% endif %}><a href="{{ path('domains') }}">Domains</a></li>
                    <li {% if 'webdesign' == active %}class="active"{% endif %}><a href="{{ path('webdesign') }}">Web Design</a></li>
                    <li {% if 'site_hosting_tools' == active %}class="active"{% endif %}><a href="{{ path('site_hosting_tools') }}">Site Hosting Tools</a></li>
                    <li {% if 'why_us' == active %}class="active"{% endif %}><a href="{{ path('why_us') }}">Why Us?</a></li>
                    <li {% if 'support' == active %}class="active"{% endif %}><a href="{{ path('support') }}">Support Center</a></li>
                </ul><!-- /.nav -->
            </div>
        </div>
    </nav>

    <div id="main" role="main">
        <div class="wrap intro {{ active|e }}">
            <div class="container">

                {# intro #}
                {% block intro %}
                {% endblock %}

            </div>
        </div><!-- /.intro -->

        {# content #}
        {% block content %}
        {% endblock %}

    </div><!-- /#main -->

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ app.request.basepath }}/js/libs/jquery-1.7.2.min.js"><\/script>')</script>
    <script src="{{ app.request.basepath }}/assets/js/scripts.js"></script>
    <footer role="contentinfo" class="wrap">
        <div class="container">
            <ul class="nav nav-list span2">
                <li class="nav-header">Web Hosting</li>
                <li><a href="#">Cheap Hosting</a></li>
                <li><a href="#">Personal/Cheap Hosting</li>
                <li><a href="{{ path('business_hosting') }}">Business Class Hosting</a></li>
                <li><a href="{{ path('vps_hosting') }}">VPS Hosting</a></li>
                <li><a href="{{ path('dedicated_servers') }}">Dedicated Servers</a></li>
                <li><a href="{{ path('domains') }}">Domain Names</a></li>
                <li><a href="{{ path('webdesign') }}">Web Design</a></li>
            </ul>
            <ul class="nav nav-list span2">
                <li class="nav-header">Site Hosting Tools</li>
                <li><a href="#">WordPress Hosting</a></li>
                <li><a href="#">Joomla Hosting</a></li>
                <li><a href="#">Drupal Hosting</a></li>
                <li><a href="#">cPanel Hosting</a></li>
                <li><a href="#">PrestaShop Hosting</a></li>
                <li><a href="#">Moodle Hosting</a></li>
                <li><a href="#">OpenCart Hosting</a><li>
            </ul>
            <ul class="nav nav-list span2">
                <li class="nav-header">Community</li>
                <li><a href="#">Support Center</a></li>
                <li><a href="#">Host Affiliate</a></li>
                <li><a href="#">Infographics</a></li>
                <li><a href="#">Los Angeles Hosting</a></li>
                <li><a href="#">Green Web Hosting</a></li>
                <li><a href="#">Student Web Hosting</a></li>
            </ul>
            <ul class="nav nav-list span2">
                <li class="nav-header">About Us</li>
                <li><a href="#">Terms of Use</a></li>
                <li><a href="#">Privacy Policy</a></li>
                <li><a href="#">Blog</a></li>
                <li><a href="#">Link to Us</a></li>
                <li><a href="#">We're Hiring</a></li>
                <li><a href="#">Site Map</a></li>
            </ul>
            <div class="span3">
                <p>Stay in touch on</p>
                <div itemscope itemtype="schema.org/LocalBusiness">
                    <h5 itemprop="name">InMotion Hosting</h5>
                    <p itemprop="address" itemscope itemtype="http://schema.org/PostalAddress">
                        <span itemprop="streetAddress">6100 Center Drive, Suite 1190</span>
                        <span itemprop="addressLocality" class="city">Los Angeles</span>, <span itemprop="addressRegion" class="state">CA</span> <span itemprop="postalCode" class="postalcode">90045</span>
                        <span itemprop="telephone">888-321-HOST(4678)</span>
                        <span>&copy; 2012 All Rights Reserved</span>
                    </p>
                </div>
            </div>
        </div>
    </footer>
</body>
</html>
